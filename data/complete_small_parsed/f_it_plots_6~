set logscale y

set terminal postscript
set title 'Search Cost in Nodes Traversed, alpha = 0.5'
set xlabel 'gamma'
set ylabel 'Search Cost in Nodes Traversed'
set output 'ham_complete_it_a0.5.ps'
plot [:34] "n20_a0.5_it.gp" with linespoints title "n=20", \
"n26_a0.5_it.gp" with linespoints title "n=26", \
"n32_a0.5_it.gp" with linespoints title "n=32", \
"n38_a0.5_it.gp" with linespoints title "n=38"

set terminal postscript
set title 'Search Cost in Nodes Traversed, alpha = 1.0'
set xlabel 'gamma'
set ylabel 'Search Cost in Nodes Traversed'
set output 'ham_complete_it_a1.0.ps'
plot [:34] "n20_a1.0_it.gp" with linespoints title "n=20", \
"n26_a1.0_it.gp" with linespoints title "n=26", \
"n32_a1.0_it.gp" with linespoints title "n=32", \
"n38_a1.0_it.gp" with linespoints title "n=38"

set terminal postscript
set title 'Search Cost in Nodes Traversed, alpha = 1.5'
set xlabel 'gamma'
set ylabel 'Search Cost in Nodes Traversed'
set output 'ham_complete_it_a1.5.ps'
plot [:34] "n20_a1.5_it.gp" with linespoints title "n=20", \
"n26_a1.5_it.gp" with linespoints title "n=26", \
"n32_a1.5_it.gp" with linespoints title "n=32", \
"n38_a1.5_it.gp" with linespoints title "n=38"
